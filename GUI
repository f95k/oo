    private void andelAActionPerformed(java.awt.event.ActionEvent evt) {                                       
        String kurskod = aField.getText();
        String a = null;
        
        try {
            a = controller.numberOfA(kurskod) + "";
            aLabel.setText(a + " %");
            
            
        } catch (Exception e) {
          aLabel.setText("Funkade ej");
        }
        
        System.out.print("Current Time in milliseconds = ");
         System.out.println(System.currentTimeMillis());
        
    }                                      

