    private void addStudentActionPerformed(java.awt.event.ActionEvent evt) {                                           
        String sPnr = sPnr1Field.getText();
        String sNamn = sNamn1Field.getText();
        String sAdress = sAdress1Field.getText();
        if(!sPnr.equals("") && !sNamn.equals("") && !sAdress.equals("")){
        try {
            controller.addStudent(sPnr, sNamn, sAdress);
            addRemoveSLabel.setForeground(Color.green);
            addRemoveSLabel.setText("Studenten registrerad!");
            
        } catch (Exception e) {
            addRemoveSLabel.setForeground(Color.red);
            addRemoveSLabel.setText("Gick ej att registrera(Student finns redan?)");
            
        }
        } else {
            addRemoveSLabel.setForeground(Color.red);
            addRemoveSLabel.setText("Alla fält måste vara ifyllda!");
            
        } 
        
         System.out.print("Current Time in milliseconds = ");
         System.out.println(System.currentTimeMillis());
        
    }                                          

    private void jButton2ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        String sPnr = sPnr1Field.getText();
        if(!sPnr.equals("")){
        try {
            controller.removeStudent(sPnr);
            addRemoveSLabel.setForeground(Color.green);
            addRemoveSLabel.setText("Studenten borttagen");
        } catch (Exception e) {
            addRemoveSLabel.setForeground(Color.red);
            addRemoveSLabel.setText("Gick ej att ta bort");
            
        } 
        } else {
            addRemoveSLabel.setForeground(Color.red);
            addRemoveSLabel.setText("Personnummer krävs!");
            
        }
        
        System.out.print("Current Time in milliseconds = ");
         System.out.println(System.currentTimeMillis());
    }                                        

    private void jButton3ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        String kurskod = kurskod1Field.getText();
        String kursnamn = kursnamn1Field.getText();
        int poang;
        try{
            poang = Integer.parseInt(poang1Field.getText());
        }catch ( Exception e){
            JOptionPane.showMessageDialog(null, "Poäng får endast bestå av heltal");
            return;
        }
        
        if(!kurskod.equals("") && !kursnamn.equals("") && !poang1Field.equals("")) {
        try {
            controller.addKurs(kurskod, kursnamn, poang);
            addKursLabel.setForeground(Color.green);
            addKursLabel.setText("Kursen registrerad");
        } catch (Exception e) {
            addKursLabel.setForeground(Color.red);
            addKursLabel.setText("Gick ej att registrera");
            
        } 
        } else {
            addKursLabel.setForeground(Color.red);
            addKursLabel.setText("Alla fält måste vara ifyllda!");
            
        }
        
         System.out.print("Current Time in milliseconds = ");
         System.out.println(System.currentTimeMillis());
    }                                        

    private void jButton4ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        String kurskod = kurskod1Field.getText();
        
        if(!kurskod.equals("")) {
        try {
            controller.removeKurs(kurskod);
            addKursLabel.setForeground(Color.green);
            addKursLabel.setText("Kursen borttagen");
        } catch (Exception e) {
            addKursLabel.setForeground(Color.red);
            addKursLabel.setText("Gick ej att ta bort");
            
        }
        } else {
            addKursLabel.setForeground(Color.red);
            addKursLabel.setText("Kurskod behövs!");
            
        }
        
        System.out.print("Current Time in milliseconds = ");
        System.out.println(System.currentTimeMillis());
    }                                        

    private void jButton5ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        String sPnr = sPnr2Field.getText();
        String kurskod = kurskod2Field.getText();
        String termin = termin1Field.getText();
        if (!sPnr.equals("") && !kurskod.equals("") && !termin.equals("")) {
        try {
            if(controller.addStudentLaser(sPnr, kurskod, termin)){
                regStudKursLabel.setForeground(Color.green);
                regStudKursLabel.setText("Studenten registrerad på kursen");
            }
           
        } catch (Exception e) {
            regStudKursLabel.setForeground(Color.red);
            regStudKursLabel.setText("Gick ej att registrera studenten på kursen");
            
        } 
            
        } else {
            regStudKursLabel.setForeground(Color.red);
            regStudKursLabel.setText("Alla fält måste vara ifyllda!");
        }
        
         System.out.print("Current Time in milliseconds = ");
         System.out.println(System.currentTimeMillis());
    }                                        

    private void jButton6ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        String sPnr = sPnr3Field.getText();
        String kurskod = kurskod3Field.getText();
        String termin = termin2Field.getText();
        String betyg = betygComboBox.getSelectedItem() + "";
        
        if(!sPnr.equals("") && !kurskod.equals("") && !termin.equals("") && !betyg.equals("")) {
        try {
            controller.addStudentLast(sPnr, kurskod, termin, betyg);
            betygLabel.setForeground(Color.green);
            betygLabel.setText("Studenten har betygsatts");
        } catch (Exception e) {
            betygLabel.setForeground(Color.red);
            betygLabel.setText("Gick ej att betygsätta");
            
        }
        } else {
            betygLabel.setForeground(Color.red);
            betygLabel.setText("Alla fält måste vara ifyllda!");
            
        }
        
        System.out.print("Current Time in milliseconds = ");
        System.out.println(System.currentTimeMillis());
    }                                        

    private void jButton7ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        String sPnr = sPnr3Field.getText();
        String kurskod = kurskod3Field.getText();
        
        if(!sPnr.equals("") && !kurskod.equals("")) {
        try {
            controller.removeStudentFromLaser(sPnr, kurskod);
            betygLabel.setForeground(Color.green);
            betygLabel.setText("Studenten borttagen från kurs!");
        } catch (Exception e) {
            betygLabel.setForeground(Color.red);
            betygLabel.setText("Gick ej att ta bort student!");
            
        }
        } else {
            betygLabel.setForeground(Color.red);
            betygLabel.setText("Personnummer och kurskod behövs!");
            
        }
        System.out.print("Current Time in milliseconds = ");
        System.out.println(System.currentTimeMillis());
    }                                        

