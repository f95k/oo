        jSeparator10.setOrientation(javax.swing.SwingConstants.VERTICAL);
        jPanel3.add(jSeparator10, new org.netbeans.lib.awtextra.AbsoluteConstraints(290, 10, 10, 150));
        jPanel3.add(studentLaserLabel, new org.netbeans.lib.awtextra.AbsoluteConstraints(70, 330, -1, -1));
        jPanel3.add(studentLastLabel, new org.netbeans.lib.awtextra.AbsoluteConstraints(370, 330, -1, -1));
        jPanel3.add(jLabel39, new org.netbeans.lib.awtextra.AbsoluteConstraints(70, 110, -1, -1));

        resultatLabel.setText("Resultat:");
        jPanel3.add(resultatLabel, new org.netbeans.lib.awtextra.AbsoluteConstraints(120, 70, -1, -1));

        jTabbedPane1.addTab("Sök 2", jPanel3);

        getContentPane().add(jTabbedPane1, new org.netbeans.lib.awtextra.AbsoluteConstraints(0, 0, 580, -1));
        getContentPane().add(jLabel1, new org.netbeans.lib.awtextra.AbsoluteConstraints(0, 0, 570, 620));

        setSize(new java.awt.Dimension(574, 639));
        setLocationRelativeTo(null);
